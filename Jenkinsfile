pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Run') {
            steps {
                echo 'Running'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Build') {
            steps {
                echo 'Building'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
